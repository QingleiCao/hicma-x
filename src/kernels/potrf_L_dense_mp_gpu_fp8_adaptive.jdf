#include "hicma_parsec.h"

/**
 * @file potrf_L_dense_mp_gpu_fp8_adaptive.jdf
 * @brief Cholesky factorization (POTRF) task graph with FP8 precision support
 * 
 * This file implements a task-based Cholesky factorization algorithm using PaRSEC runtime
 * with enhanced support for FP8 (8-bit floating point) precision. It extends the base
 * implementation with additional precision conversion capabilities and FP8-specific optimizations.
 * 
 * Key features:
 * - Full FP8 precision support with conversion routines
 * - Mixed precision computation (DP, SP, HP, FP8)
 * - GPU acceleration with CUDA/HIP support including FP8 kernels
 * - Low-rank compression for off-diagonal blocks
 * - Enhanced memory pool management for FP8 data types
 * - Dynamic task scheduling with priority-based execution
 * 
 * FP8 Support:
 * - FP8 data type definitions and conversions
 * - GPU kernels optimized for FP8 arithmetic
 * - Memory-efficient FP8 storage and communication
 */

/* 
 * Helper routines for task pool initialization and key generation:
 * 1. undetermined_nb_tasks: Returns PARSEC_UNDETERMINED_NB_TASKS to allow dynamic task creation
 * 2. my_make_key_potrf_dgemm: Creates unique keys for GEMM tasks to ensure proper task ordering
 * 
 * These functions are essential for the PaRSEC runtime to properly manage task dependencies
 * and ensure correct execution order in the Cholesky factorization algorithm.
 */
static uint32_t undetermined_nb_tasks(struct __parsec_potrf_L_dense_mp_gpu_fp8_adaptive_internal_taskpool_s *__tp);
static parsec_key_t my_make_key_potrf_dgemm(const parsec_taskpool_t * tp, const parsec_assignment_t * as);

/**
 * Task Priority System:
 * 
 * The priority system ensures that tasks are executed in the correct order for Cholesky factorization.
 * Higher priority values indicate tasks that should be executed earlier.
 * 
 * Priority formulas:
 * - potrf_dpotrf(k)    : (MT-k)**3     - Diagonal block factorization
 * - potrf_dsyrk(k,m)   : (MT-m)**3 + 3 * (m - k)     - Rank-k update
 * - potrf_dtrsm(m,k)   : (MT-m)**3 + 3 * (m - k) * (2 * MT - k - m - 1)     - Triangular solve
 * - potrf_dgemm(m,n,k) : (MT-m)**3 + 3 * (m - n) * (2 * MT - m - n - 1) + 6 * (m - k)     - Matrix multiply
 *
 * Maximum priority calculation:
 * (MT - PRI_CHANGE)**3 + 3 * MT * (2 * MT - PRI_CHANGE - 1) + 6 * MT  < (MT**3 + 6 MT**2 + 3 MT)
 *
 * WARNING: If mt (matrix tile count) is greater than 1200, integer overflow may occur.
 */

%}

%option nb_local_tasks_fn = undetermined_nb_tasks

/**
 * Global Variables:
 * These variables are shared across all tasks in the task graph
 */
descA        [ type = "parsec_tiled_matrix_t*" ]        /* Main matrix descriptor for the input matrix A */
descAr       [ type = "parsec_tiled_matrix_t*" aligned = descA ]  /* Rank matrix descriptor for low-rank compression */
descRank     [ type = "parsec_tiled_matrix_t*" aligned = descA ]  /* Matrix descriptor for rank information storage */
descNorm     [ type = "parsec_tiled_matrix_t*" aligned = descA ]        /* Norm descriptor used for norm */
descFake     [ type = "parsec_tiled_matrix_t*" ]        /* Fake descriptor used for termination tasks */
params_tlr   [ type = "hicma_parsec_params_t *" ]       /* TLR (Tile Low-Rank) parameters and configuration */

/**
 * Hidden Global Variables:
 * These variables are internal to the task pool and not exposed to external users
 */
/* Memory pool handlers for different data types and operations */
p_work       [ type = "parsec_memory_pool_t *" hidden = on default = NULL ]  /* General purpose work memory pool */
p_work_rr    [ type = "parsec_memory_pool_t *" hidden = on default = NULL ]  /* Memory pool for rank reduction operations */
p_work_mbr   [ type = "parsec_memory_pool_t *" hidden = on default = NULL ]  /* Memory pool for matrix block operations */
p_work_full_dp        [ type = "parsec_memory_pool_t *" hidden = on default = NULL ]  /* Double precision full matrix memory pool */
p_work_full_sp        [ type = "parsec_memory_pool_t *" hidden = on default = NULL ]  /* Single precision full matrix memory pool */
p_work_full_hp        [ type = "parsec_memory_pool_t *" hidden = on default = NULL ]  /* Half precision full matrix memory pool */
p_work_full_fp8       [ type = "parsec_memory_pool_t *" hidden = on default = NULL ]  /* FP8 precision full matrix memory pool */
p_work_uv_dp          [ type = "parsec_memory_pool_t *" hidden = on default = NULL ]  /* Double precision U/V factor memory pool */
p_work_uv_sp          [ type = "parsec_memory_pool_t *" hidden = on default = NULL ]  /* Single precision U/V factor memory pool */

/* Task priority control variables */
PRI_CHANGE   [ type = "int" hidden = on default = 0 ]   /* Priority change threshold for task scheduling */
PRI_MAX      [ type = "int" hidden = on default = "(descA->mt * ( 3 + descA->mt * ( 2 + descA->mt )))" ]  /* Maximum priority value */

/* GPU-related variables */
ws_gpu       [ type = "void *" hidden = on default = NULL ]  /* GPU workspace memory */
nb_cuda_devices      [ type = "int"   hidden = on default = 0 ]  /* Number of available CUDA devices */
cuda_device_index    [ type = "int *" hidden = on default = "NULL"]  /* Array of CUDA device indices for load balancing */


/**************************************************
 *               Define Arena                     *
 **************************************************/
/**
 * Arena definition for memory management and data type handling.
 * This arena defines the memory pools and data types used throughout
 * the Cholesky factorization algorithm, including support for:
 * - Double precision (FULL_DP)
 * - Single precision (FULL_SP) 
 * - Half precision (FULL_HP)
 * - FP8 precision (FULL_FP8)
 * - Low-rank U/V factors (UV_DP, UV_SP)
 * - Rank information (AR)
 * - Communication buffers (BYTE)
 */
my_arena(k)

// Execution space - single iteration for arena initialization
k = 0 .. 0 

// Parallel partitioning - binds to the main matrix descriptor
:descA(0, 0)

// Memory pool parameters for different data types and operations
READ A1 <- NULL       [ type = FULL_DP ]    /* Double precision full matrix memory pool */
READ A2 <- NULL       [ type = FULL_SP ]    /* Single precision full matrix memory pool */
READ A3 <- NULL       [ type = FULL_HP ]    /* Half precision full matrix memory pool */
READ A4 <- NULL       [ type = UV_DP ]      /* Double precision U/V factor memory pool */
READ A5 <- NULL       [ type = UV_SP ]      /* Single precision U/V factor memory pool */
READ A6 <- NULL       [ type = BYTE ]       /* Communication buffer memory pool */
READ A7 <- NULL       [ type = AR ]         /* Rank information memory pool */
READ A8 <- NULL       [ type = FULL_FP8 ]   /* FP8 precision full matrix memory pool */

BODY
{
    /* Arena initialization - no specific operations needed */
}
END


/**************************************************
 *               potrf_bind_A                     *
 **************************************************/
/**
 * Matrix binding task for Cholesky factorization.
 * This task binds matrix tiles to the appropriate computational tasks
 * based on their position in the matrix and the algorithm requirements.
 * 
 * Task routing logic:
 * - (0,0): First diagonal block -> POTRF factorization
 * - (m,0): First column blocks -> TRSM triangular solve
 * - (m,m): Diagonal blocks -> SYRK rank-k update
 * - (m,n): Off-diagonal blocks -> GEMM matrix multiplication
 */
potrf_bind_A(m, n)

// Execution space - iterate over all matrix tiles
m = 0 .. descA->mt-1
n = %{ return parsec_imax(m-params_tlr->band_size_dense+1, 0); %} .. m

// Parallel partitioning - bind to matrix descriptor
:descA(m, n)

// Data flow routing based on tile position
READ A <- descA(m, n)       
       -> (m == 0 && n == 0) ? T potrf_dpotrf(0)                      [ type_remote = FULL_DP ]
       -> (n == 0)? C potrf_dtrsm(m, n)                               [ type_remote = %{ return &__parsec_tp->super.arenas_datatypes[decision_datatype_tile_potrf_L_dense_mp_gpu_fp8_adaptive(params_tlr->decisions, m, n, descA->lmt)]; %} ] 
       -> (m == n && n > 0) ? T potrf_dsyrk(0, m)                     [ type_remote = FULL_DP ]
       -> (m != n && n > 0) ? C potrf_dgemm(m, n, 0)                  [ type_remote = %{ return &__parsec_tp->super.arenas_datatypes[decision_datatype_tile_potrf_L_dense_mp_gpu_fp8_adaptive(params_tlr->decisions, m, n, descA->lmt)]; %} ] 

BODY
{
#if defined(PARSEC_HAVE_DEV_CUDA_SUPPORT)  || defined(PARSEC_HAVE_DEV_HIP_SUPPORT)
    /* GPU load balancing and data placement optimization */
    if( nb_cuda_devices > 0 ) {
        int g = gpu_load_balance_2d( m, n, params_tlr );
        parsec_advise_data_on_device( _f_A->original,
                                    cuda_device_index[g],
                                    PARSEC_DEV_DATA_ADVICE_PREFERRED_DEVICE );
    }
#endif
}
END


/**************************************************
 *                  TERMINATE                     *
 **************************************************/
/**
 * Termination task for Cholesky factorization.
 * This task is executed on each node after the last diagonal block
 * factorization is completed, signaling the end of the computation.
 * 
 * The task sets the number of remaining tasks to 1, which allows
 * the PaRSEC runtime to properly terminate the task pool.
 */
TERMINATE(n)
// Execution space - one task per compute node
n = 0 .. descA->super.nodes-1

// Use fake descriptor for termination
:descFake(0, n)

// Control dependency - wait for the last POTRF task to complete
CTL ctl <- ctl potrf_dpotrf(descA->mt-1)

BODY
{
    /* Signal completion by setting task count to 1 */
    __parsec_tp->super.super.tdm.module->taskpool_set_nb_tasks(&__parsec_tp->super.super, 1);
#if DEBUG_INFO
    fprintf(stderr, "Rank %d in TERMINATE\n", n);
#endif
}
END


/**************************************************
 *               potrf_dpotrf                     *
 **************************************************/
/**
 * Cholesky factorization of diagonal blocks (POTRF).
 * This task performs the Cholesky factorization of the k-th diagonal block.
 * It is the core computational kernel that decomposes a symmetric positive
 * definite matrix into L*L^T form.
 * 
 * The task supports multiple execution backends:
 * - GPU execution (CUDA/HIP) for large blocks
 * - Recursive CPU execution for medium blocks
 * - Sequential CPU execution for small blocks
 * 
 * Priority: High priority to ensure diagonal blocks are processed first
 */
potrf_dpotrf(k) [high_priority = on]

// Execution space - one task per diagonal block
k = 0 .. descA->mt-1

info = 0  /* Return code for recursive calls */

// Parallel partitioning - operates on diagonal blocks
:descA(k, k)

// Data dependencies and flow
RW T <- (k == 0) ? A potrf_bind_A(k, k) : T potrf_dsyrk(k-1, k)   [ type_remote = FULL_DP ] 
     -> T0 potrf_dpotrf_send(k)                                   [ type_remote = FULL_DP ] 
     -> descA(k, k)                       

// Termination control - last diagonal block triggers termination
CTL ctl -> (k == descA->mt-1)? ctl TERMINATE(0 .. descA->super.nodes-1)

// Dynamic priority calculation for task scheduling
; (k >= (descA->mt - PRI_CHANGE)) ? (descA->mt - k) * (descA->mt - k) * (descA->mt - k) : PRI_MAX

BODY [type=CUDA]
{
#if defined(PARSEC_HAVE_DEV_CUDA_SUPPORT)
    /* GPU-accelerated Cholesky factorization using CUDA */
    hicma_parsec_core_potrf_gpu( descA, params_tlr, ws_gpu, cuda_device, gpu_task, cuda_stream, T, k );
#if ENABLE_PROFILING
    /* Stop profiling after processing a few blocks for performance analysis */
    if( 10 == k) cudaProfilerStop();
#endif
#endif
}
END

BODY [type=HIP]
{
#if defined(PARSEC_HAVE_DEV_HIP_SUPPORT)
    /* GPU-accelerated Cholesky factorization using HIP (AMD ROCm) */
    hicma_parsec_core_potrf_gpu( descA, params_tlr, ws_gpu, cuda_device, gpu_task, cuda_stream, T, k );
#endif
}
END

BODY [type=RECURSIVE]
{
    /* Recursive CPU execution for medium-sized blocks */
    int tempkm = k == descA->mt-1 ? descA->m - k*descA->mb : descA->mb;
    int smallnb = params_tlr->HNB;

    /* Use sequential CPU version for small blocks */
    if( tempkm <= smallnb ) { 
        return PARSEC_HOOK_RETURN_NEXT;
    }

    if(DEBUG_INFO) printf("POTRF_Recursive: %d\n", k);

    /* Count floating-point operations for performance monitoring */
    unsigned long int cnt = hicma_parsec_op_counts('c', tempkm, 0, 0, 0);
    params_tlr->op_band[es->th_id] += cnt;
    params_tlr->op_path[es->th_id] += cnt;

    /* Print progress information */
    hicma_parsec_print_process( descA->mt, k, params_tlr->start_time_potrf );

    /* Create subtile descriptor for recursive call */
    subtile_desc_t *small_descT;
    parsec_taskpool_t *parsec_dpotrf;

    small_descT = subtile_desc_create( descA, k, k,
            smallnb, smallnb, 0, 0, tempkm, tempkm );
    small_descT->mat = T;

    /* Create recursive POTRF task pool */
    parsec_dpotrf = dplasma_dpotrf_New(params_tlr->uplo, (parsec_tiled_matrix_t *)small_descT, &this_task->locals.info.value);

    /* Execute recursive call asynchronously */
    parsec_recursivecall((parsec_task_t*)this_task,
            parsec_dpotrf, dplasma_dpotrf_Destruct,
            1, small_descT);

    return PARSEC_HOOK_RETURN_ASYNC;
}
END

BODY
{
    /* Sequential CPU execution for small blocks or fallback */
    hicma_parsec_core_potrf_cpu( descA, params_tlr, es, T, k );
}
END


/**************************************************
 *               potrf_dpotrf_send                *
 **************************************************/
/**
 * Precision conversion and data sending for POTRF results.
 * This task handles precision conversion from double precision to the
 * target precision (single, half, or FP8) and prepares data for
 * subsequent TRSM operations.
 * 
 * The task performs:
 * - Precision conversion when needed (DP -> SP/HP/FP8)
 * - Data preparation for triangular solve operations
 * - Memory management for converted data
 */
potrf_dpotrf_send(k) [high_priority = on]

// Execution space - one task per diagonal block
k = 0 .. descA->mt-1

// Parallel partitioning - operates on diagonal blocks
:descA(k, k)

// Data flow with conditional precision conversion
READ T0 <- T potrf_dpotrf(k)                                                                                               [ type_remote = FULL_DP ]
        -> (%{ return params_tlr->decisions_send[k*params_tlr->NT+k] == DENSE_DP; %})? T potrf_dtrsm(k+1..descA->lmt-1, k) [ type_remote = FULL_DP ]

RW T    <- (%{ return params_tlr->decisions_send[k*params_tlr->NT+k] != DENSE_DP; %})? NEW: NULL                           [ type = FULL_SP ]
        -> (%{ return params_tlr->decisions_send[k*params_tlr->NT+k] != DENSE_DP; %})? T potrf_dtrsm(k+1..descA->lmt-1, k) [ type_remote = FULL_SP ] 
; (k >= (descA->mt - PRI_CHANGE)) ? (descA->mt - k) * (descA->mt - k) * (descA->mt - k) : PRI_MAX


BODY [type=CUDA]
{
#if defined(PARSEC_HAVE_DEV_CUDA_SUPPORT)
    /* GPU-accelerated precision conversion from double to single precision */
    if( params_tlr->decisions_send[k*params_tlr->NT+k] != DENSE_DP ) {
        double2float_GPU( descA->mb, descA->nb, T0, descA->mb, T, descA->mb, cuda_stream->cuda_stream );
        this_task->data._f_T.data_out->original->nb_elts = descA->mb * descA->nb * sizeof(float);
    }
#endif
}
END

BODY [type=HIP]
{
#if defined(PARSEC_HAVE_DEV_HIP_SUPPORT)
    /* HIP-accelerated precision conversion from double to single precision */
    if( params_tlr->decisions_send[k*params_tlr->NT+k] != DENSE_DP ) {
        double2float_GPU( descA->mb, descA->nb, T0, descA->mb, T, descA->mb, cuda_stream->cuda_stream );
        this_task->data._f_T.data_out->original->nb_elts = descA->mb * descA->nb * sizeof(float);
    } 
#endif
}
END

BODY
{
    /* CPU-based precision conversion from double to single precision */
    if( params_tlr->decisions_send[k*params_tlr->NT+k] != DENSE_DP ) {
        LAPACKE_dlag2s( LAPACK_COL_MAJOR, descA->mb, descA->nb, T0, descA->mb, T, descA->mb );
        this_task->data._f_T.data_out->original->nb_elts = descA->mb * descA->nb * sizeof(float);
    }
}
END



/**************************************************
 *               potrf_dtrsm                      *
 **************************************************/
/**
 * Triangular solve operation (TRSM) for Cholesky factorization.
 * This task performs the triangular solve L*X = B, where L is the
 * lower triangular factor from POTRF and B is the input matrix block.
 * 
 * The task supports:
 * - Multiple precision formats (DP, SP, HP, FP8)
 * - Low-rank compression for off-diagonal blocks
 * - GPU acceleration for dense blocks
 * - Recursive CPU execution for medium blocks
 * - Sequential CPU execution for small blocks
 */
potrf_dtrsm(m, k) [high_priority = on]

// Execution space - off-diagonal blocks below the diagonal
m = 1 .. descA->mt-1
k = 0 .. m-1

// Control message size for rank information
size = 1

// Local copy of band size for GPU evaluation
band_size_dense_local = params_tlr->band_size_dense


// Parallel partitioning
: descA(m, k)

// Parameters
READ  T <- (%{ return params_tlr->decisions_send[k*params_tlr->NT+k] == DENSE_DP; %})? T0 potrf_dpotrf_send(k)  [ type_remote = FULL_DP ] 
        <- T potrf_dpotrf_send(k)                                                                                 [ type_remote = FULL_SP ]

RW    C <- (k == 0 && m-k < params_tlr->band_size_dense) ? A potrf_bind_A(m, k) [ type_remote = %{ return &__parsec_tp->super.arenas_datatypes[decision_datatype_tile_potrf_L_dense_mp_gpu_fp8_adaptive(params_tlr->decisions, m, k, descA->lmt)]; %} ]
        <- (k == 0 && m-k >= params_tlr->band_size_dense) ? descA(m, k)         
        <- C potrf_dgemm(m, k, k-1)                                             [ type_remote = %{ return &__parsec_tp->super.arenas_datatypes[decision_datatype_tile_potrf_L_dense_mp_gpu_fp8_adaptive(params_tlr->decisions, m, k, descA->lmt)]; %} ]
        -> A potrf_dsyrk(k, m)                                                  [ type_remote = BYTE layout_remote = MPI_BYTE count_remote = size ]
        -> C0 potrf_dtrsm_send(m, k)                                            [ type_remote = BYTE layout_remote = MPI_BYTE count_remote = size ]  
        -> descA(m, k)                                                         

READ  Cr <- (k == 0 && m-k >= params_tlr->band_size_dense) ? descAr(m, k)  
         <- (k != 0 && m-k >= params_tlr->band_size_dense) ? Cr potrf_dgemm(m, k, k-1): NULL   [ type_remote = AR ]
         -> (m-k >= params_tlr->band_size_dense) ? Ar potrf_dsyrk(k, m)                        [ type_remote = AR ]
         -> (m-k >= params_tlr->band_size_dense) ? Ar potrf_dgemm(m, k+1..m-1, k)              [ type_remote = AR ]
         -> (m-k >= params_tlr->band_size_dense) ? Br potrf_dgemm(m+1..descA->mt-1, m, k)      [ type_remote = AR ]
         -> (m-k >= params_tlr->band_size_dense) ? descAr(m, k)               

RW  C_norm <- (params_tlr->adaptive_decision_runtime)? descNorm(m, k): NULL
           -> (params_tlr->adaptive_decision_runtime)? A_norm potrf_dgemm(m, k+1..m-1, k)              [ type_remote = NORM ]
           -> (params_tlr->adaptive_decision_runtime)? B_norm potrf_dgemm(m+1..descA->mt-1, m, k)      [ type_remote = NORM ]
           //-> descNorm(m, k)

CTL ctl  <- (params_tlr->lookahead == 1 && m > params_tlr->lookahead+k)? ctl potrf_dsyrk(k, k+1)
CTL ctl1 <- (params_tlr->lookahead > 1 && m > params_tlr->lookahead+k)? ctl2 potrf_dtrsm(k+2, k)
CTL ctl2 -> (params_tlr->lookahead > 1 && m == k+2)? ctl1 potrf_dtrsm(params_tlr->lookahead+k+1 .. descA->mt-1, k)

CTL ctl_left  -> (params_tlr->left_looking > 0)? ctl_left potrf_dgemm(m, k+params_tlr->Q*params_tlr->left_looking, 0)


; (m >= (descA->mt - PRI_CHANGE)) ? (descA->mt - m) * (descA->mt - m) * (descA->mt - m) + 3 * ((2 * descA->mt) - k - m - 1) * (m - k) : PRI_MAX


BODY [type=CUDA]
{
#if defined(PARSEC_HAVE_DEV_CUDA_SUPPORT)
    /* Skip GPU execution for low-rank blocks */
    if ( !IS_DENSE(m, k) ) {
        return PARSEC_HOOK_RETURN_NEXT;
    }

    parsec_potrf_stream_workspace_t *stream_found = lookup_gpu_workspace(cuda_device, cuda_stream, ws_gpu);
    cublasSetStream( stream_found->handle_cublas, cuda_stream->cuda_stream );

    /* GPU-accelerated triangular solve using CUDA */
    hicma_parsec_core_trsm_gpu( descA, params_tlr, stream_found, cuda_device, gpu_task, cuda_stream, T, C, m, k );

    /* Update communication size based on data type */
    /* Calculate F norm */
    //cublasSetPointerMode(stream_found->handle_cublas, CUBLAS_POINTER_MODE_DEVICE);
    if( DENSE_DP == params_tlr->decisions[k*descA->lmt+m] ) {
        if(params_tlr->adaptive_decision_runtime) {
            double C_norm_dp = 0.0;
            cublasDnrm2(stream_found->handle_cublas, descA->mb * descA->mb, C, 1, &C_norm_dp);
            //printf("%d %d: norm %lf\n", m, k, C_norm_dp); 
            cudaMemcpy(C_norm, &C_norm_dp, sizeof(double), cudaMemcpyHostToDevice);
        }
        this_task->locals.size.value = descA->mb * descA->mb * sizeof(double);
    } else { 
        if(params_tlr->adaptive_decision_runtime) {
            float C_norm_sp = 0.0f;
            cublasSnrm2(stream_found->handle_cublas, descA->mb * descA->mb, C, 1, &C_norm_sp);
            copy_float_to_device_double(C_norm, C_norm_sp, cuda_stream->cuda_stream);
            //printf("%d %d: norm %f\n", m, k, C_norm_sp); 
        }
        this_task->locals.size.value = descA->mb * descA->mb * sizeof(float);
    }
#endif
}
END

BODY [type=HIP]
{
#if defined(PARSEC_HAVE_DEV_HIP_SUPPORT)
    /* Skip GPU execution for low-rank blocks */
    if ( !IS_DENSE(m, k) ) {
        return PARSEC_HOOK_RETURN_NEXT;
    }

    parsec_potrf_stream_workspace_t *stream_found = lookup_gpu_workspace(cuda_device, cuda_stream, ws_gpu);
    cublasSetStream( stream_found->handle_cublas, cuda_stream->cuda_stream );

    /* GPU-accelerated triangular solve using HIP (AMD ROCm) */
    hicma_parsec_core_trsm_gpu( descA, params_tlr, stream_found, cuda_device, gpu_task, cuda_stream, T, C, m, k );

    /* Update communication size based on data type */
    /* Calculate F norm */
    //cublasSetPointerMode(stream_found->handle_cublas, CUBLAS_POINTER_MODE_DEVICE);
    if( DENSE_DP == params_tlr->decisions[k*descA->lmt+m] ) {
        if(params_tlr->adaptive_decision_runtime) {
            double C_norm_dp = 0.0;
            cublasDnrm2(stream_found->handle_cublas, descA->mb * descA->mb, C, 1, &C_norm_dp);
            //printf("%d %d: norm %lf\n", m, k, C_norm_dp);
            cudaMemcpy(C_norm, &C_norm_dp, sizeof(double), cudaMemcpyHostToDevice);
        }
        this_task->locals.size.value = descA->mb * descA->mb * sizeof(double);
    } else {  
        if(params_tlr->adaptive_decision_runtime) {
            float C_norm_sp = 0.0f;
            cublasSnrm2(stream_found->handle_cublas, descA->mb * descA->mb, C, 1, &C_norm_sp);
            copy_float_to_device_double(C_norm, C_norm_sp, cuda_stream->cuda_stream);
            //printf("%d %d: norm %f\n", m, k, C_norm_sp);
        }
        this_task->locals.size.value = descA->mb * descA->mb * sizeof(float); 
    }
#endif
}
END

BODY [type=RECURSIVE]
{
    /* Recursive CPU execution for medium-sized dense blocks */
    int tempmm = m == descA->mt-1 ? descA->m - m * descA->mb : descA->mb;
    int smallnb = params_tlr->HNB;

    /* Use sequential CPU version for small blocks or non-dense blocks */
    if( tempmm <= smallnb || DENSE_DP != params_tlr->decisions[k*descA->lmt+m] ) {
        return PARSEC_HOOK_RETURN_NEXT;
    }

    if(DEBUG_INFO) printf("TRSM Recursive: %d %d ; mb %d, nb: %d, band_size_dense: %d\n", m, k, descA->mb, descA->nb, params_tlr->band_size_dense);
    
    /* Create subtile descriptors for recursive call */
    subtile_desc_t *small_descT;
    subtile_desc_t *small_descC;

    small_descT = subtile_desc_create( descA, k, k,
            smallnb, smallnb, 0, 0, descA->mb, descA->mb );
    small_descT->mat = T;

    small_descC = subtile_desc_create( descA, m, k,
            smallnb, smallnb, 0, 0, tempmm, descA->mb );
    small_descC->mat = C;

    /* Create recursive TRSM task pool */
    parsec_taskpool_t* parsec_dtrsm = dplasma_dtrsm_New(PlasmaRight, PlasmaLower,
            PlasmaTrans, PlasmaNonUnit,
            (double)1.0,
            (parsec_tiled_matrix_t *)small_descT,
            (parsec_tiled_matrix_t *)small_descC );

    /* Execute recursive call asynchronously */
    parsec_recursivecall((parsec_task_t*)this_task,
            parsec_dtrsm, dplasma_dtrsm_Destruct,
            2, small_descT, small_descC );

    /* Update communication size for double precision */
    this_task->locals.size.value = descA->mb * descA->mb * sizeof(double);

    /* Count floating-point operations */
    hicma_parsec_op_count_trsm( descA, params_tlr, m, k, es->th_id, tempmm, IS_DENSE(m, k)? 0: ((int*)Cr)[0] );

    return PARSEC_HOOK_RETURN_ASYNC;
}
END

BODY
{
    /* Skip computation for zero-rank low-rank blocks */
    if( !IS_DENSE(m, k) && 0 == *((int *)Cr) ) {
        this_task->locals.size.value = 0;
        return PARSEC_HOOK_RETURN_DONE;
    }

    /* Sequential CPU execution for small blocks or low-rank blocks */
    hicma_parsec_core_trsm_cpu( descA, descRank, params_tlr, es,
            p_work_full_sp, T, C, m, k, IS_DENSE(m, k)? 0: ((int*)Cr)[0] );

    /* Update communication size based on data type and rank */
    if( DENSE_DP == params_tlr->decisions[k*descA->lmt+m] ) {
        this_task->locals.size.value = descA->mb * descA->mb * sizeof(double);
        ((double *)C_norm)[0] = LAPACKE_dlange(LAPACK_COL_MAJOR, 'F', descA->mb, descA->nb, C, descA->mb);
    } else if( DENSE_SP == params_tlr->decisions[k*descA->lmt+m] || DENSE_HP == params_tlr->decisions[k*descA->lmt+m] || DENSE_FP8 == params_tlr->decisions[k*descA->lmt+m] ) {
        this_task->locals.size.value = descA->mb * descA->mb * sizeof(float);
        float C_norm_sp = LAPACKE_slange(LAPACK_COL_MAJOR, 'F', descA->mb, descA->nb, C, descA->mb);
        ((double *)C_norm)[0] = C_norm_sp; 
    } else if( LOW_RANK_DP == params_tlr->decisions[k*descA->lmt+m] ) {
        this_task->locals.size.value = descA->mb * ((int *)Cr)[0] * 2 * sizeof(double);
    } else { 
        this_task->locals.size.value = descA->mb * ((int *)Cr)[0] * 2 * sizeof(float);
    }

}
END


/**************************************************
 *               potrf_dtrsm_send                 *
 **************************************************/
/**
 * Precision conversion and data sending for TRSM results.
 * This task handles precision conversion from the computation precision
 * to the target precision for subsequent GEMM operations.
 * 
 * The task performs:
 * - Precision conversion (DP -> SP/HP/FP8) when needed
 * - Data preparation for matrix multiplication operations
 * - Memory management for converted data
 * - Communication size calculation based on target precision
 */
potrf_dtrsm_send(m, k) [high_priority = on]

// Execution space - off-diagonal blocks below the diagonal
m = 1 .. descA->mt-1
k = 0 .. m-1

// Control message size for rank information
size = 1

// Local copy of band size for GPU evaluation
band_size_dense_local = params_tlr->band_size_dense


// Parallel partitioning
: descA(m, k)

// Parameters
READ  C0 <- C potrf_dtrsm(m, k)                                                                       [ type_remote = %{ return &__parsec_tp->super.arenas_datatypes[decision_datatype_tile_potrf_L_dense_mp_gpu_fp8_adaptive(params_tlr->decisions, m, k, descA->lmt)]; %} ] 
        -> (%{ return !hicma_parsec_convert_in_trsm(params_tlr, m, k); %})? A potrf_dgemm(m, k+1..m-1, k)          [ type_remote = BYTE layout_remote = MPI_BYTE count_remote = size ] 
        -> (%{ return !hicma_parsec_convert_in_trsm(params_tlr, m, k); %})? B potrf_dgemm(m+1..descA->mt-1, m, k)  [ type_remote = BYTE layout_remote = MPI_BYTE count_remote = size ] 

RW    C <- (%{ return hicma_parsec_convert_in_trsm(params_tlr, m, k); %})? NEW: NULL                               [ type = %{ return &__parsec_tp->super.arenas_datatypes[decision_datatype_tile_send_potrf_L_dense_mp_gpu_fp8_adaptive(params_tlr->decisions_send, m, k, descA->lmt)]; %} ]
        -> (%{ return hicma_parsec_convert_in_trsm(params_tlr, m, k); %})? A potrf_dgemm(m, k+1..m-1, k)           [ type_remote = BYTE layout_remote = MPI_BYTE count_remote = size ]
        -> (%{ return hicma_parsec_convert_in_trsm(params_tlr, m, k); %})? B potrf_dgemm(m+1..descA->mt-1, m, k)   [ type_remote = BYTE layout_remote = MPI_BYTE count_remote = size ]

; (m >= (descA->mt - PRI_CHANGE)) ? (descA->mt - m) * (descA->mt - m) * (descA->mt - m) + 3 * ((2 * descA->mt) - k - m - 1) * (m - k) : PRI_MAX


BODY [type=CUDA]
{
#if defined(PARSEC_HAVE_DEV_CUDA_SUPPORT)
    /* Skip GPU execution for low-rank blocks */
    if ( !IS_DENSE(m, k) ) {
        return PARSEC_HOOK_RETURN_NEXT;
    }

    /* Calculate communication size based on target precision */
    if( DENSE_DP == params_tlr->decisions_send[k*descA->lmt+m] ) {
        this_task->locals.size.value = descA->mb * descA->mb * sizeof(double);
    } else if( DENSE_SP == params_tlr->decisions_send[k*descA->lmt+m] ) {
        this_task->locals.size.value = descA->mb * descA->mb * sizeof(float);
